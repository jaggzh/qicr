<?xml version="1.0" encoding="utf-8"?>

<inset xmlns:android="http://schemas.android.com/apk/res/android"
       android:insetTop="@dimen/standard_margin"
       android:insetBottom="@dimen/standard_margin"
       android:insetLeft="@dimen/standard_margin"
       android:insetRight="@dimen/standard_margin"
        >
    <shape
            android:shape="rectangle"
            android:thickness="1dp"
            android:useLevel="false">
        <corners android:radius="1dp"/>
        <stroke
                android:color="#ff555588"
                android:width="1dp"/>
        <solid android:color="#ccb8b8b8"/>
    </shape>
</inset>
